def handles_if(a: bool) -> int {
  if a {
    4
  } else {
    5
  }
}

def main(a: bool) -> int {
  let b = 20 - 10;
  (1 + 2) * (50 - 36) / 6 + b;
  handles_if(a)
}

def add(a: int, b: int) -> int {
  a + b
}

def no_return(a: int, b: int) -> int {
  a + b;
}

pub def double_add(a: int, b: int,) -> int {
  add(a, b) + add(a, b)
}
